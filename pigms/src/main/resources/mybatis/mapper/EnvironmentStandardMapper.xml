<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhu.mapper.EnvironmentStandardMapper">
    
    <resultMap id="BaseResultMap" type="com.zhu.pojo.EnvironmentStandard">
        <id column="id" property="id"/>
        <result column="parameter_name" property="parameterName"/>
        <result column="min_value" property="minValue"/>
        <result column="max_value" property="maxValue"/>
        <result column="unit" property="unit"/>
        <result column="description" property="description"/>
    </resultMap>

    <select id="selectByPrimaryKey" parameterType="java.lang.Integer" resultMap="BaseResultMap">
        SELECT * FROM tb_environment_standard WHERE id = #{id}
    </select>

    <select id="selectByParameterName" parameterType="java.lang.String" resultMap="BaseResultMap">
        SELECT * FROM tb_environment_standard WHERE parameter_name = #{parameterName}
    </select>

    <select id="selectAll" resultMap="BaseResultMap">
        SELECT * FROM tb_environment_standard ORDER BY id ASC
    </select>

</mapper>



